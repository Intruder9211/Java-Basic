package Arrays;

public class Arrays {
    
    public static void main(String[] args) {
        int[] marks = new int[3];
        marks[0]= 97;//phy
        marks[1]= 95;//che
        marks[2]= 93;//math
        System.out.println(marks[0]);
       // System.out.println(marks[0]);
        System.out.println(marks[1]);
        System.out.println(marks[2]);
    }
}
